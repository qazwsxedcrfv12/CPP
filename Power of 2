#include<bits/stdc++.h>
using namespace std;
/*---------------------------------Direct Method----------------------------*/
int power_of_two(int n)
{
   int c;
   c = n && (!(n & (n-1));
  return c;
}

int main()
{
       int n,p;
   cin>>n;
        p = power_of_two(n);
   if(p==1)
       cout<<"Power of: "<<2;
   else
       cout<<"Not Power of: "<<2;
  return 0;
}
/*-----------------------------------------------------------------------------*/

/*--------------------------------Uisng Loop-----------------------------------*/
int main()
{
   int n,c=0;
      cin>>n;
   while( n != 0)
   {
       c += 1;
       n = n/10;
   }
   
   if(c==1)
       cout<<"Power of: "<<2;
   else
       cout<<"Not Power of: "<<2;
  return 0; 
}
/*------------------------------------------------------------------------------*/
