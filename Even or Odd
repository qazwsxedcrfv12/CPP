#include<bits/stdc++.h>
using namespace std;

/* Using bit-wise operators ---------------------------------------------------*/
int check(int n)
{
     if(n & 1 == 0)
       return 1;
   return 0;
}

int main()
{
    int n;
        cin>>n;
    p=check(n);
        if(n==1)
          cout<<"Even";
        else
          cout<<"Odd";
  return 0;
}

/*--------------------------------------------------------------------------------*/

/* Using Modulo operator----------------------------------------------------------*/
int main()
{
    int n;
        cin>>n;
        if(n%2==0)
          cout<<"Even";
        else
          cout<<"Odd";
  return 0;
}
/*---------------------------------------------------------------------------------*/
