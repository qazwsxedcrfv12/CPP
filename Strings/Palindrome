/******************************Check a string is Palindrome or not*********************************/
#include<bits/stdc++.h>
using namespace std;

int main()
{

   ios_base::sync_with_stdio(false); 
   cin.tie(NULL);  
      
   string str;
   int l,flag=0;
   
   cin>>str;
   l=str.length();
   
   for(int i=0; i<l/2; i++)
   {
       if(str[i]!=str[l-i-1])
       {
           flag=1;
           break;
       }
   }
   if(flag!=0)
     cout<<"Not a Palindrome"<<endl;
   else
     cout<<"Palindrome"<<endl;
 return 0;
}
/****************************************************************************************************/
