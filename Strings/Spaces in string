#include<bits/stdc++.h>
using namespace std;
/****************************************Using Direct Formula*************************************************/
int main()
{
    string str;
    getline(cin,str);
    
    str.erase(remove(s.begin(),s.end(),' '),s.end()));
    
    cout<<str;
   return 0;
}

/*************************************************************************************************************/

/*****************************************Using Naive Solution************************************************/

int main()
{
    string str;
    getline(cin,str);
    
    int i = 0, j = 0;
    while(str[i])
    {
        if(str[i] != ' ')
          str[j++] = str[i];
        i++;
    }
    str[j] = '\0';
     cout<<str;
    return 0;
  }
  /***********************************************************************************************************/
